{
    "name": "cwsps154/app-settings",
    "description": "Package for saving app settings",
    "keywords": [
        "cwsps154",
        "laravel",
        "filament",
        "filament-packages",
        "filament-plugins",
        "filament-app-settings"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CWSPS154\\AppSettings\\": "src/",
            "CWSPS154\\AppSettings\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "authors": [
        {
            "name": "CWSPS154",
            "email": "codewithsps154@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2",
        "ysfkaya/filament-phone-input": "^3.1",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CWSPS154\\AppSettings\\AppSettingsServiceProvider"
            ]
        }
    }
}
